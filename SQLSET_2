create table Orders1 (
order_number int ,
customer_number int ,
primary key(order_number)
);

insert into Orders1 values(1 , 1),
(2 , 2),
(3 , 3),
(4  ,3)


select customer_number  from Orders1
 group by customer_number
 order by count(*) desc limit 1 ;
 
 
 
 
