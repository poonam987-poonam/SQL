create  table Employee (
employee_id int ,
team_id int ,
primary key(employee_id)
);

insert into  Employee values (1 , 8),
(2 , 8),
(3 , 8),
(4 , 7),
(5 , 9),
(6 , 9)




select employee_id , 
count(team_id) over(partition by team_id) as team_size 
from Employee


